<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="rootPane" prefHeight="161.0" prefWidth="290.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SettingsController">
   <children>
      <TextField fx:id="username" layoutX="52.0" layoutY="28.0" prefHeight="25.0" prefWidth="186.0" promptText="New Member ID..." />
      <Button fx:id="save" layoutX="52.0" layoutY="122.0" mnemonicParsing="false" onAction="#handleSave" text="Save" />
      <Button fx:id="cancel" layoutX="186.0" layoutY="122.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
      <PasswordField fx:id="password" layoutX="52.0" layoutY="68.0" prefHeight="25.0" prefWidth="186.0" promptText="New Password..." />
   </children>
</AnchorPane>
